{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": ["node", "vite/client", "vue", "@pinia/nuxt", "nuxt/schema"]
  },
  "include": [
    ".nuxt/types/**/*.ts",
    "**/*.ts",
    "**/*.d.ts",
    "**/*.tsx",
    "**/*.vue",
    ".nuxt/nuxt.d.ts"
  ],
  "exclude": ["node_modules", ".output", "dist"]
}
